<table class="ZDAGlanceList" lang="en">

<% if (!locals.ZDAGlanceListData.length) { -%>
	<p class="ZDAGlanceListEmpty"><%= OLSKLocalized('ZDAGlanceListEmptyText') %></p>
<% } -%>

<% if (locals.ZDAGlanceListData.length) { -%>
<thead>
	<tr class="ZDAGlanceListHead OLSKStickyHeader OLSKCommonEdgeBottom">
		<th class="ZDAGlanceListHeadName ZDAGlanceListSort" data-sort="ZDAGlanceListItemName"><%= OLSKLocalized('ZDAGlanceListHeadNameText') %></th>
		<th class="ZDAGlanceListHeadRemoteStorage ZDAGlanceListSort" data-sort="ZDAGlanceListItemRemoteStorage">remoteStorage</th>
		<th class="ZDAGlanceListHeadFission ZDAGlanceListSort" data-sort="ZDAGlanceListItemFission">Fission</th>
		<th class="ZDAGlanceListHeadSOLID ZDAGlanceListSort" data-sort="ZDAGlanceListItemSOLID">SOLID</th>
	</tr>
</thead>
<% } -%>

<tbody class="list">

<% locals.ZDAGlanceListData.forEach(function (item) { -%>
<tr class="ZDAGlanceListItem">
	<td class="OLSKCommonCard">
		<% if (item.ZDAProjectIconURL) { -%>
			<a class="ZDAGlanceListItemIcon" href="<%= item.ZDAProjectURL %>" aria-hidden="true" tabindex="-1">
				<img class="ZDAGlanceListItemIconImage" src="<%= item._ZDAProjectIconURLCachedPath || item.ZDAProjectIconURL %>" />
			</a>
		<% } -%>
		<section>
			<a class="ZDAGlanceListItemName" href="<%= item.ZDAProjectURL %>" target="_blank"><%= item.ZDAProjectName %></a>
			<span class="ZDAGlanceListItemBlurb"><%= item.ZDAProjectBlurb %></span>
		</section>
	</td>
	<td class="ZDAGlanceListItemCheckbox">
		<input class="ZDAGlanceListItemRemoteStorage" type="checkbox" disabled <%= item.ZDAProjectBanks.ZDABankRemoteStorage ? 'checked' : '' %> data-boolean="<%= item.ZDAProjectBanks.ZDABankRemoteStorage ? '0' : '1' %>">
	</td>
	<td class="ZDAGlanceListItemCheckbox">
		<input class="ZDAGlanceListItemFission" type="checkbox" disabled <%= item.ZDAProjectBanks.ZDABankFission ? 'checked' : '' %> data-boolean="<%= item.ZDAProjectBanks.ZDABankFission ? '0' : '1' %>">
	</td>
	<td class="ZDAGlanceListItemCheckbox">
		<input class="ZDAGlanceListItemSOLID" type="checkbox" disabled <%= item.ZDAProjectBanks.ZDABankSolidProject ? 'checked' : '' %> data-boolean="<%= item.ZDAProjectBanks.ZDABankSolidProject ? '0' : '1' %>">
	</td>
</tr>
<% }) -%>

</tbody>

</table>
