<div class="ZDAVitrineList">

<% if (!locals.ZDAVitrineListData.length) { -%>
	<p class="ZDAVitrineListEmpty"><%= OLSKLocalized('ZDAVitrineListEmptyText') %></p>
<% } -%>

<% locals.ZDAVitrineListData.forEach(function (item) { -%>
<div class="ZDAVitrineListItem OLSKCommonCard">
	<div>
		<a class="ZDAVitrineListItemName" href="<%= item.ZDAProjectURL %>"><%= item.ZDAProjectName %></a>

		<span class="ZDAVitrineListItemBlurb"><%= item.ZDAProjectBlurb %></span>
	</div>
</div>
<% }) -%>

</div>
